{
  "name": "my-stuff",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/dmitry-honcharuk/my-stuff.git",
  "author": "dmitry.honcharuk <dmitry.honcharuk@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "concurrently -n=client,server,docker -c=cyan,magenta,green \"yarn client:start\" \"yarn server:start\" \"yarn docker:start\"",
    "start:app": "concurrently -n=client,server -c=cyan,magenta \"yarn client:start\" \"yarn server:start\"",
    "server:start": "webpack --config webpack.server.config",
    "client:start": "webpack-dev-server -d --config webpack.client.config",
    "docker:up": "docker-compose up -d",
    "docker:start": "docker-compose start",
    "docker:stop": "docker-compose stop",
    "docker:down": "docker-compose down",
    "migration:generate": "sequelize migration:generate --name",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo",
    "test": "node ./core/config/loaded-env && echo $TEST"
  },
  "workspaces": [
    "core/*",
    "client/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "^4.6.0",
    "eslint": "^5.14.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.5.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jsonwebtoken": "^8.5.0",
    "lint-staged": "^8.1.4",
    "mysql2": "^1.6.5",
    "nodemon-webpack-plugin": "^4.0.7",
    "prettier": "^1.16.4",
    "remove-webpack-plugin": "^1.2.2",
    "sequelize-cli": "^5.4.0",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14",
    "webpack-node-externals": "^1.7.2"
  }
}
