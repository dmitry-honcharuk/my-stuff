{
  "name": "my-stuff",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/dmitry-honcharuk/my-stuff.git",
  "author": "dmitry.honcharuk <dmitry.honcharuk@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "yarn docker:up",
    "start:app": "concurrently -n=client,server -c=cyan,magenta \"yarn start:client\" \"yarn start:server\"",
    "start:server": "webpack --config webpack/webpack.server.dev.config",
    "start:client": "webpack-dev-server -d --config webpack/webpack.client.dev.config",
    "build": "docker build -t dmitryhoncharuk/multi-client ./client",
    "build:server": "webpack --config webpack/webpack.server.prod.config",
    "build:client": "webpack --config webpack/webpack.client.prod.config",
    "docker:up": "docker-compose up",
    "docker:start": "docker-compose start",
    "docker:stop": "docker-compose stop",
    "docker:down": "docker-compose down",
    "migration:generate": "sequelize migration:generate --name",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo",
    "migrate:undo:all": "sequelize db:migrate:undo:all"
  },
  "workspaces": [
    "core/*",
    "client/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "concurrently": "^4.1.2",
    "copy-webpack-plugin": "^5.0.4",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.5",
    "jsonwebtoken": "^8.5.0",
    "lint-staged": "^9.2.5",
    "mysql2": "^1.7.0",
    "nodemon-webpack-plugin": "^4.1.1",
    "prettier": "^1.18.2",
    "remove-webpack-plugin": "^1.2.2",
    "sequelize-cli": "^5.5.1",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0",
    "webpack-node-externals": "^1.7.2"
  }
}
